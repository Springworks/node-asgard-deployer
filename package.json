{
  "name": "asgard-deployer",
  "version": "1.2.3",
  "description": "Executes automated deployments in Asgard",
  "main": "deploy.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf lib && babel src --out-dir lib",
    "lint": "eslint .",
    "test": "NODE_ENV=test istanbul cover _mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "bin": {
    "asgard-deployer": "./bin/cli.js"
  },
  "engines": {
    "npm": ">=2.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Springworks/node-asgard-deployer.git"
  },
  "author": "Springworks",
  "license": "MIT",
  "devDependencies": {
    "@springworks/test-harness": "1.0.3",
    "babel": "5.8.23",
    "babel-eslint": "4.1.7",
    "coveralls": "2.11.4",
    "eslint": "1.5.1",
    "eslint-config-springworks": "3.1.1",
    "eslint-plugin-mocha": "1.0.0",
    "eslint-plugin-should-promised": "1.0.5",
    "istanbul": "0.3.21",
    "mocha": "2.3.3"
  },
  "dependencies": {
    "@springworks/error-factory": "2.0.3",
    "@springworks/api-client": "5.1.0",
    "@springworks/input-validator": "3.1.1",
    "bluebird": "2.10.2",
    "bunyan": "1.5.1"
  }
}
