{
  "name": "asgard-deployer",
  "version": "1.2.4",
  "description": "Executes automated deployments in Asgard",
  "main": "deploy.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rm -rf lib && babel src --out-dir lib",
    "lint": "eslint --cache .",
    "test": "NODE_ENV=test istanbul cover _mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "bin": {
    "asgard-deployer": "./bin/cli.js"
  },
  "engines": {
    "npm": ">=2.7.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Springworks/node-asgard-deployer.git"
  },
  "author": "Springworks",
  "license": "MIT",
  "devDependencies": {
    "@springworks/test-harness": "1.5.1",
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.0",
    "babel-plugin-transform-strict-mode": "6.18.0",
    "babel-preset-es2015-node4": "2.1.0",
    "babel-register": "6.18.0",
    "coveralls": "2.11.14",
    "eslint": "3.10.2",
    "eslint-config-springworks": "7.0.6",
    "eslint-plugin-import": "2.1.0",
    "eslint-plugin-mocha": "4.7.0",
    "eslint-plugin-should-promised": "2.0.0",
    "eslint-plugin-springworks": "2.0.1",
    "istanbul": "0.4.5",
    "mocha": "3.1.2"
  },
  "dependencies": {
    "@springworks/error-factory": "2.0.9",
    "@springworks/api-client": "6.1.6",
    "@springworks/input-validator": "4.0.27",
    "bluebird": "3.4.6",
    "bunyan": "1.8.4"
  }
}
